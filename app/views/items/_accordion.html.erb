<div class="accordion" id="accordionExample">
    <div class="accordion-item" data-bs-toggle="collapse">
      <h2 class="accordion-header">
        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne" style="background-image: url('https://images.unsplash.com/photo-1584269600464-37b1b58a9fe7?q=80&w=2071&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D'); background-size: cover; background-position: center center">
          Rice
        </button>
      </h2>
      <div id="collapseOne" class="accordion-collapse collapse" data-bs-parent="#accordionExample">
        <div class="accordion-body">
          <div class="row">
            <% @rice_items.each do |item| %>
              <div class="col-6">
                <div class="entry">
                  <%= image_tag item.photo, alt: "text" %>
                  <h6><%= item.name %></h6>
                  <div class="price">
                    <h5>Rp <%= item.price %></h5>
                  </div>
                  <button class="button" data-cart-number-target='addButton' data-action='click->cart-number#addToCart'
                    data-item-id="<%= item.id %>">Add to cart
                  </button>
                </div>
              </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
